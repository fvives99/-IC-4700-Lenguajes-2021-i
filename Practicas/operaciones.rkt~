#lang racket

(define (sumar)
  (+ (get-number) (get-number)))

(define (restar)
  (- (get-number) (get-number)))

(define (mutiplicar)
  (* (get-number) (get-number)))

(define (dividir)
  (/ (get-number) (get-number)))

(define (get-number)
  (display "Ingrese un nÃºmero: ")
  (strig->number(read-line(current-input-port) *any)))